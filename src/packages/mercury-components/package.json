{
  "name": "mercury",
  "version": "0.1.3",
  "peerDependencies": {
    "prop-types": ">=15",
    "react": ">=16.2.0"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:es",
    "build:lib": "rimraf lib && babel src -d lib --source-maps",
    "build:es": "rimraf es && cross-env BABEL_ENV=es babel src -d es --source-maps"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "author": "Simon Moore",
  "license": "MIT",
  "description": "Mercury project specific components",
  "private": false,
  "publishConfig": {
    "access": "public"
  }
}
